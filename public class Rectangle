public class Rectangle()
    {
         private double length;
         private double height;
         private double area;
         
         public void setLength(double length)
         {
             this.length = length;
         }
         public void setLength(double height)
         {
             this.height = height; 
         }
         public void setArea()
         {
             area = length * height;
             System.println.area;
         }

    }
